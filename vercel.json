{
    "version": 2,
    "installCommand": "yarn install",
    "buildCommand": "cd packages/database && yarn build && cd ../nest-js && yarn build && cd ../../apps/microservices/auth && yarn build",
    "outputDirectory": "apps/microservices/auth/dist",
    "builds": [
        {
            "src": "apps/microservices/auth/dist/main.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/auth/(.*)",
            "dest": "apps/microservices/auth/dist/main.js"
        }
    ]
}